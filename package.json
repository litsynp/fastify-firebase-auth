{
  "name": "fastify-firebase-auth",
  "version": "0.0.1",
  "description": "Practicing Fastify with Firebase Auth",
  "main": "src",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --ext '*' --watch src --exec 'ts-node src/index.ts' --delay 2",
    "test": "jest -i",
    "test:watch": "jest -i --watch",
    "lint:es": "eslint '**/*.{ts,tsx}'",
    "lint:es:fix": "eslint '**/*.{ts,tsx}' --ext .ts --fix",
    "lint:etc": "prettier '**/*.{js,ts,tsx,json,yaml,md}' --check",
    "lint:etc:fix": "prettier '**/*.{js,ts,tsx,json,yaml,md}' --write",
    "lint": "npm run lint:es && npm run lint:etc",
    "lint:fix": "npm run lint:es:fix && npm run lint:etc:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/litsynp/fastify-firebase-auth.git"
  },
  "keywords": [
    "fastify",
    "firebase",
    "auth",
    "firebase-auth",
    "typescript",
    "node"
  ],
  "author": "litsynp",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/litsynp/fastify-firebase-auth/issues"
  },
  "homepage": "https://github.com/litsynp/fastify-firebase-auth#readme",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
